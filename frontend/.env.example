# URL for the API used by the frontend (exposed to the client)
NEXT_PUBLIC_API_URL=http://localhost:8080/api

# MongoDB connection URI for your database
MONGODB_URI=your_mongo_db_uri

# Secret key used for signing JWT tokens (should be kept private)
JWT_SECRET=your_jwt_secret

# Public payment API key used for integrating payment services
NEXT_PUBLIC_PAYMENT_API_KEY=your_payment_api_key
